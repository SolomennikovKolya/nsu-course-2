<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nsu.nikvel.tetris.control.gui.scenes.MainMenuController">
    <children>
        <VBox layoutX="92.0" layoutY="49.0">
            <children>
                <Button fx:id="buttonNewGame" mnemonicParsing="false" onAction="#clickButtonNewGame" prefWidth="116.0" text="New Game" />
                <Button fx:id="buttonHighScores" mnemonicParsing="false" onAction="#clickButtonHighScores" prefHeight="25.0" prefWidth="116.0" text="High Scores" />
            <Button fx:id="buttonAbout" mnemonicParsing="false" onAction="#clickButtonAbout" prefHeight="25.0" prefWidth="116.0" text="About" />
                <Button fx:id="buttonExit" mnemonicParsing="false" onAction="#clickButtonExit" prefHeight="25.0" prefWidth="116.0" text="Exit" />
            </children>
        </VBox>
    </children>
</AnchorPane>
